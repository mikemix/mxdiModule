{
    "name": "mikemix/mxdi-module",
    "description": "Configure dependency injection in ZF2 using annotations/YAML",
    "keywords": [
        "zf2",
        "zendframework",
        "di",
        "dependency injection",
        "annotations",
        "yaml"
    ],
    "homepage": "https://github.com/mikemix/mxdiModule",
    "license": "MIT",
    "authors": [
        {
            "name": "Michal Zukowski",
            "homepage": "https://www.phpcontext.com"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "doctrine/annotations": "~1.2.4",
        "symfony/yaml": "~2.7.1",
        "zendframework/zend-servicemanager": "~2.5",
        "zendframework/zend-modulemanager": "~2.5",
        "zendframework/zend-mvc": "~2.5",
        "zendframework/zend-cache": "~2.5",
        "zendframework/zend-console": "~2.5",
        "ocramius/proxy-manager": "~1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.8"
    },
    "autoload": {
        "psr-4": {
            "mxdiModule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mxdiModuleTest\\": "test"
        }
    }
}
