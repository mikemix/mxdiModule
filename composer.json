{
    "name": "mikemix/mxdiModule",
    "description": "Configure dependency injection using annotations",
    "keywords": [
        "zf2",
        "di",
        "dependency injection",
        "annotations"
    ],
    "homepage": "https://github.com/mikemix/mxdiModule",
    "license": "MIT",
    "authors": [
        {
            "name": "Michal Zukowski",
            "homepage": "https://www.phpcontext.com"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "doctrine/common": "~1.2.4",
        "zendframework/zend-servicemanager": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "mxdiModule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mxdiModuleTest\\": "test"
        }
    }
}
