{
    "name": "mikemix/mxdi-module",
    "description": "Configure dependency injection using annotations",
    "keywords": [
        "zf2",
        "zendframework",
        "di",
        "dependency injection",
        "annotations"
    ],
    "homepage": "https://github.com/mikemix/mxdiModule",
    "license": "MIT",
    "authors": [
        {
            "name": "Michal Zukowski",
            "homepage": "https://www.phpcontext.com"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "doctrine/annotations": "~1.2.4",
        "zendframework/zend-servicemanager": "~2.5",
        "zendframework/zend-cache": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "~3.7"
    },
    "autoload": {
        "psr-4": {
            "mxdiModule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mxdiModuleTest\\": "test"
        }
    },
    "suggest": {
        "ocramius/proxy-manager": "Create lazy ZF2 services"
    }
}
